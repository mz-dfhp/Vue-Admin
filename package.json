{
  "name": "vue-admin",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev",
    "build": "vue-tsc -b && vite build",
    "build:test": "vue-tsc && vite build --mode test",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:prod": "vue-tsc && vite build --mode prod",
    "build:github": "vue-tsc && vite build --mode github --base=/Vue-Admin/",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "element-plus": "^2.7.6",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "uuid": "^10.0.0",
    "vue": "^3.4.29",
    "vue-router": "4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.22.0-beta.3",
    "@iconify-json/bi": "^1.1.23",
    "@iconify/tailwind": "^1.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.10",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
